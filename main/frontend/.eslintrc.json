{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_" }],
    "no-console": "warn",
    "prettier/prettier": "error"
  },
  "plugins": [
    "prettier"
  ]
}

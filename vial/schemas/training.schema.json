{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": { "type": "string", "pattern": "^vial_[0-9a-f]{6}$" },
    "input": { "type": "string" },
    "model": { "type": "string", "enum": ["default", "3d", "ai_model"] },
    "epochs": { "type": "integer", "minimum": 1 },
    "latency": { "type": "number", "minimum": 0 },
    "codeLength": { "type": "integer", "minimum": 0 },
    "agentId": { "type": "string", "pattern": "^(agent|ai)_[0-9a-f]{6}$" }
  },
  "required": ["id", "input", "model", "epochs", "latency", "codeLength"]
}

// Rebuild Instructions: Place in /vial/schemas/. Ensure used by training.js. Run Troubleshoot in vial.html to check for errors.

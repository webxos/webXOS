  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.5",
    "body-parser": "^1.20.2",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "pyodide": "^0.26.2"
  },
  "devDependencies": {
    "emscripten": "^3.1.61"
  }
}

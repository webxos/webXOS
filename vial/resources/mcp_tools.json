{
  "tools": [
    {
      "name": "nomic_search",
      "agent": "agent1",
      "endpoint": "/v1/api/nomic_search",
      "description": "Vector-based search using Nomic embeddings",
      "enabled": true,
      "config": {
        "api_key": "${NOMIC_API_KEY}",
        "model": "nomic-embed-text-v1",
        "limit": 5
      }
    },
    {
      "name": "cognitallmware_search",
      "agent": "agent2",
      "endpoint": "/v1/api/cognitallmware_search",
      "description": "Vector-based search using CogniTALLMware embeddings",
      "enabled": true,
      "config": {
        "api_key": "${COGNITAL_API_KEY}",
        "model": "cognitallmware-v1",
        "limit": 5
      }
    },
    {
      "name": "llmware_search",
      "agent": "agent3",
      "endpoint": "/v1/api/llmware_search",
      "description": "Vector-based search using LLMware embeddings",
      "enabled": true,
      "config": {
        "api_key": "${LLMWARE_API_KEY}",
        "model": "llmware-embed-v1",
        "limit": 5
      }
    },
    {
      "name": "jinaai_search",
      "agent": "agent4",
      "endpoint": "/v1/api/jinaai_search",
      "description": "Vector-based search using Jina AI embeddings",
      "enabled": true,
      "config": {
        "api_key": "${JINAAI_API_KEY}",
        "model": "jina-embeddings-v2",
        "limit": 5
      }
    },
    {
      "name": "git_operations",
      "endpoint": "/v1/api/git",
      "description": "Execute Git commands for repository management",
      "enabled": true,
      "config": {
        "repo_url": "https://github.com/webxos/webxos.git",
        "allowed_commands": ["git clone", "git commit", "git push", "git pull", "git branch", "git merge", "git checkout", "git diff"]
      }
    }
  ]
}

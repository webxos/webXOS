{
  "version": 2,
  "builds": [
    {
      "src": "main/pages/**/*",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main/pages/$1"
    }
  ],
  "env": {
    "JWT_SECRET_KEY": "your_jwt_secret_key",
    "REDIS_HOST": "redis",
    "REDIS_PORT": "6379",
    "API_KEY": "api-a24cb96b-96cd-488d-a013-91cb8edbbe68"
  }
}
